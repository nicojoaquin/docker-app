{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn ttsc",
    "start": "node build/index.js",
    "develop": "NODE_PATH=src ts-node-dev src/index.ts",
    "dev": "docker-compose up -d --build && docker-compose logs -f",
    "stop": "docker kill $(docker ps -q)",
    "clean": "docker rm $(docker ps -a -q) && docker volume prune -f",
    "reset": "yarn stop && yarn clean",
    "restart": "yarn stop && yarn dev",
    "lint": "eslint . --ext .ts",
    "typeorm": "typeorm-ts-node-esm -d ./src/ormConfig",
    "migration:generate": "npm run typeorm migration:generate ./src/migrations/migrations && npm run build",
    "migration:up": "npm run typeorm migration:run",
    "migration:down": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "dotenv-override": "^5.0.1",
    "eslint": "^8.23.1",
    "express": "^4.18.1",
    "mysql2": "^2.3.3",
    "typeorm": "^0.3.9",
    "typescript-transform-paths": "^3.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.18",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.3"
  }
}
